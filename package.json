{
  "name": "nest-template",
  "version": "0.0.1",
  "description": "",
  "author": "gylove1994",
  "private": true,
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand --passWithNoTests",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "p:migrate:dev": "dotenv -e .env.development prisma migrate dev",
    "p:reset:dev": "dotenv -e .env.development prisma migrate reset",
    "pretest": "dotenv -e .env.test prisma migrate reset --force",
    "prebuild": "rm -rf dist",
    "p:studio:dev": "dotenv -e .env.development prisma studio",
    "p:studio:prod": "dotenv -e .env.production prisma studio",
    "p:studio:test": "dotenv -e .env.test prisma studio",
    "p:check:dev": "dotenv -e .env.development prisma validate && dotenv -e .env.development prisma migrate reset -f",
    "p:check:test": "dotenv -e .env.test prisma validate && dotenv -e .env.test prisma migrate reset -f",
    "p:run": "prisma migrate deploy",
    "doc:gen": "compodoc -p tsconfig.json",
    "doc:serve": "compodoc -p tsconfig.json -s",
    "env:start:dev": "docker-compose -f docker-compose-dev.yml up -d",
    "env:stop:dev": "docker-compose -f docker-compose-dev.yml down",
    "lint-staged": "lint-staged",
    "p:seed": "prisma db seed",
    "prepare": "husky"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.4.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "5.17.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.3",
    "cache-manager-ioredis-yet": "^2.1.1",
    "chalk": "4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nest-winston": "^1.10.0",
    "nestjs-i18n": "^10.4.5",
    "nestjs-minio": "^2.6.2",
    "nestjs-pino": "^4.1.0",
    "nestjs-prisma": "^0.23.0",
    "nodemailer": "^6.9.15",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "prisma-class-generator": "^0.2.9",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.3.10",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "helmet": "^7.1.0",
    "husky": "^9.1.5",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.0.0",
    "prettier-eslint": "^16.3.0",
    "prisma": "^5.16.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ]
  }
}
